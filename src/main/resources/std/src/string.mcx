import {
  int::-,
  int::=,
}

export builtin def size : func (_ : string) -> int;

export const rec def repeat : func (s : string, count : int) -> string :=
  \(s, count) -> if (count = 0) then "" else (s ++ repeat(s, count - 1));

export def to_string : func (_ : string) -> string := $(identity(string_tag))(string);
