import {
  int::-,
  int::=,
}

export builtin def size: (_: String) -> Int;

export const def repeat: (string: String, count: Int) -> string =
  \(string, count) -> if count = 0 then "" else string ++ repeat(string, count - 1);

export def to_string: String -> String = $identity(StringTag, `String);
