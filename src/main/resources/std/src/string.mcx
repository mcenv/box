import {
  int::-,
  int::=,
}

export builtin def size : Func (_ : String) -> Int;

export const rec def repeat : Func (string : String, count : Int) -> String :=
  \(string, count) -> if (count = 0) then "" else (string ++ repeat(string, count - 1));

export def to_string : Func (_ : String) -> String := $(identity(StringTag))(String);
