import {
  int::=,
}

def id : proc (x : i32) -> i32 := \(x) -> x;

def apply_def : i32 := id(-1);

def apply_proc : i32 := (\(x) -> x)(-1);

def apply_func : i32 := (\\(x) -> x)(-1);

test def test_apply_def := apply_def = -1;

test def test_apply_proc := apply_proc = -1;

test def test_apply_func := apply_func = -1;
